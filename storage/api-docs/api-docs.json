                "responses": {
                    "200": {
                        "description": "Получаем JWT token",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "access_token": {
                                            "type": "string",
                                            "example": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9"
                                        },
                                        "token_type": {
                                            "type": "string",
                                            "example": "bearer"
